{
  "expo": {
    "name": "react-native-fiber-firebase",
    "slug": "react-native-fiber-firebase",
    "description": "UI elements, multi-screen implemented in React Native, now in one handy kit",
    "version": "2.0.0",
    "orientation": "portrait",
    "privacy": "public",
    "icon": "./assets/app.png",
    "primaryColor": "#00AAFF",
    "splash": {
      "image": "./assets/OMEGAPOGGERS.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wcandillon.fiber",
      "buildNumber": "2"
    },
    "androidStatusBar": {
      "backgroundColor": "#ffffff",
      "barStyle": "dark-content"
    },
    "android": {
      "package": "com.wcandillon.fiber",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
